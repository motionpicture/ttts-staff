<%
const locales = conf.get('locales');
let routeName = '';
if (req.hasOwnProperty('route')) {
    routeName = req.route.path;
}
%>
<header>
    <div class="container">

        <a href="javascript:void(0);" class="header-logo"><img src="/images/common/logo.png" alt="TTTS"></a>

        <div class="header-menu">
    
        </div>

        <div class="header-lang">
            <p>Language</p>
            <select name="locale" id="select_locale">
                <% Object.keys(locales).forEach(function (locale) { %>
                    <option value="<%= locale %>"><%= locales[locale] %></option>
                <% }); %>
            </select>
        </div>
    </div>
</header>
